<EmberAnchor @a={{anchor}} />
<ApiIndexFilter @model={{model}} @filterData={{filterData}} as |filteredModel|>
  {{#each filteredModel.properties as |property|}}
    <ClassFieldDescription @type="property" @field={{property}} @model={{model}} @updateAnchor={{action "updateAnchor"}} />
  {{/each}}
</ApiIndexFilter>
